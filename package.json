{
    "name": "eddy",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "deploy": "ng build --base-href /",
        "client-s3-deploy": "aws s3 sync dist/eddy/browser/ s3://eddy.diamondshub.it/ --delete",
        "client-cloudfront-invalidation": "aws cloudfront create-invalidation --distribution-id E19XW11BEMBK4Z  --paths '/*' ",
        "client-deploy": "npm run deploy && npm run client-s3-deploy && npm run client-cloudfront-invalidation"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.1.1",
